      <h4> WasteNot </h4>
    </div>
    <div class="p-2 bd-highlight">
      <%= link_to "https://github.com/AdamPriestman/wastenot" do %>
        <i class="project-github-link fab fa-github"></i>
      <% end %>
    </div>
    <div class="p-2 bd-highlight">
      <%= render 'modal_project_2' %>
