        <%= image_tag "4.jpeg", class: "d-block w-100", alt:"..." %>
      </div>
      <div class="carousel-item">
        <%= image_tag "5.jpeg", class: "d-block w-100", alt:"..." %>
      </div>
      <div class="carousel-item">
        <%= image_tag "6.jpeg", class: "d-block w-100", alt:"..." %>
      </div>
      <div class="carousel-item">
        <%= image_tag "7.jpeg", class: "d-block w-100", alt:"..." %>
      </div>
    </div>
